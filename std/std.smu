(type string string/t)

(defn print [str] (string/puts (array-data str)))

(type (option 'a) (option/t 'a))

-- char
(defn char-equal [a b] (= (u8_to_int a) (u8_to_int b)))

(defn iter-range [from to f]
  (defn inner [i]
    (if (= i to)
      ()
      (do (f i) (inner (+ i 1)))))
  (inner from))

(module array array)
