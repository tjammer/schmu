(rule
 (targets std.smi std.o array.smi array.o string.smi string.o)
 (deps std.smu %{bin:schmu} array.smu string.smu)
 (action
  (progn
   (run schmu -m --no-std array.smu)
   (run schmu -m --no-std string.smu)
   (run schmu -m --no-std std.smu))))

(install
 (section
  (site
   (schmu std)))
 (files std.smu std.smi std.o array.smu array.smi array.o string.smi string.o))
