external printi : int -> unit

type t('a) = { null : int, first : int, gen : 'a, third : bool }

type gen_first('a) = { only : 'a, is : bool }

function print_bool(b)
  if b then printi(1)
  else printi(0)
  end
end

function first(any)
  printi(any.first)
end

function gen(any)
  any.gen
end

function third(any)
  print_bool(any.third)
end

function only(any)
  any.only
end

function is(any)
  print_bool(any.is)
end

int_t = { null = 0, first = 700, gen = 20, third = true }
f = { only = 420, is = false }

first(int_t)
third(int_t)
printi(gen(int_t))
printi(only(f))
is(f)
