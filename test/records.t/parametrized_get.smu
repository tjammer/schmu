external printi : int -> unit

type 'a t = { null : int, first : int, gen : 'a, third : bool }

function first(any)
  printi(any.first)

function gen(any)
  any.gen

function third(any)
  if any.third then printi(1)
  else printi(0)

int_t = { null = 0, first = 700, gen = 20, third = true }

first(int_t) >>
third(int_t) >>
printi(gen(int_t))
