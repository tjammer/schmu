type state = {cnt : int, next : (int) -> int}

fun advance(state!) {
  {cnt = (state.next)(state.cnt), next = state.next}
}
fun rec ten_times(state!) {
  if state.cnt < 10 {
    fmt/(println(int, state.cnt))
    ten_times(!advance(!state))
  } else {
    fmt/(println(int, 100))
  }
}
{
  let state = {cnt = 0, next = fun x {x + 1}}
  ten_times(!state)
}
