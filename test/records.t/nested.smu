external printi : int -> unit

type innerst('b) = { a : 'b }
type p_inner('a) = { a : 'a }
type t('a) = { x : int, inner : p_inner(innerst('a)) }

type inner = { a : int }

type foo = { a : int, b : inner }

function inner()
  { a = 3 }
end

f = fn(x) x end

a = { a = 0, b = inner() }
printi(a.b.a)
printi(f({ x = 17, inner = { a = { a = 124 } } }).inner.a.a)
