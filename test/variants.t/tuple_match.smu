external printf : (string/cstr, int) -> unit

fun doo(a, b):
  match(a, b):
    (Some(a), Some(b)): a + b
    (None, Some(b)): b
    (Some(a), None): a
    (_, None): 0
  |> printf(string/data("%i\n"))

let none_int : option(int) = None

doo(Some(1), Some(2))
doo(none_int, Some(2))
doo(Some(1), None)
doo(none_int, None)
