(external printf (fun cstr int unit))

(defn doo [a b]
  (->> (match {a b}
         ({(#some a) (#some b)} (+ a b))
         ({#none (#some b)} b)
         ({(#some a) #none} a)
         ({_ #none} 0))
       (printf (array-data "%i\n"))))

(def (none_int (option int)) #none)

(doo (#some 1) (#some 2))
(doo none_int (#some 2))
(doo (#some 1) #none)
(doo none_int #none)
