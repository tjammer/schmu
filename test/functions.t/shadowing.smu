external printi : int -> unit

fn fib(n)
    if n < 2 then n
    else
        -- local function
        fn fibn2(n)
            fib(n - 2)
        -- anonymous function
        -- shadow fib. Since the function is anonymous, the inner fib call still calls the outer fib
        fib = fn (n)
            fib(n - 1)
        fibn2(n) + fib(n)

printi(fib(30))
