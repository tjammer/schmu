external printi : int -> unit

type t('a) = { x : 'a }

fn apply(x, f , env)
  f(x, env)

fn apply2(x : 'a, f : ('a, 'a -> 'b) -> 'b, env : 'a -> 'b)
  f(x, env)

fn add1(x : int) -> int
  x + 1

fn boxed2int_int(t, env)
  a = env(t.x)
  { x = a }

a = apply({x = 15}, boxed2int_int, add1).x
printi(a) >>
b = apply2({x = 15}, boxed2int_int, add1).x
printi(b)
