external printi : fun (int) -> unit

type t['a] = {x : 'a}

fun apply(x, f, env) {f(x, env)}

fun apply2(x : 'a, f : fun ('a, fun ('a) -> 'b) -> 'b, env : fun ('a) -> 'b) {
  f(x, env)
}

fun add1(x : int) {x + 1}

fun boxed2int_int(t, many env) {
  let a = env(t.x)
  {x = a}
}

let a = apply({x = 15}, boxed2int_int, add1).x
printi(a)
let b = apply2({x = 15}, boxed2int_int, add1).x
printi(b)
