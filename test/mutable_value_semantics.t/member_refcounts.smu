(type r {:a (array int)})
(def a& [10])
(def r {:a a})
(array-set &a 0 20)
(print (fmt-str (array-get (.a r) 0)))

(def r [a])
(set &(array-get a 0) 30)
(print (fmt-str (array-get (array-get r 0) 0)))

(def r (#some a))
(set &(array-get a 0) 40)
(print
 (match r
   ((#some a) (fmt-str (array-get a 0)))
   (#none "none")))
