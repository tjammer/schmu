(signature
  (type t int)
  (type abstr)

  (def t int)
  (def ext (fun string/cstr int unit))
  (def int (fun int unit))
  (def poly-int (fun 'a 'a)))

(external ext (fun string/cstr int unit) "printf")

(defn int ([i int])
  (print (fmt-str i)))

(defn poly-int (a) (copy a))

-- type t is usable here
(def [t t] 200)

(defn hidden (a)
  (print (fmt-str (+ 2 a))))

(type hidden-type int)

(type abstr int)
