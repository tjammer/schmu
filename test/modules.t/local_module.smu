fun test() {println("hey test")}

module local {
  signature {
    type t
    val test : () -> unit
    val poly_test : (fmt/stdout/printer['a], 'a) -> unit
    val value : string
  }
  module nested {
    type t = unit
  }
  type t = int
  fun test() {
    println("hey thing")
  }
  fun poly_test(pr, a) {
    fmt/print1("hey poly {}\n", pr, a)
  }
  let value = "test"
  ignore(value)
}
test()

local/test()
local/poly_test(fmt/str, "test")

