-- use polymoorphic aliased function from another module
fun swapped(mov a, i, j) {
  let mut a = mov a
  swap(mut a, i, j)
  a
}
array/iter(swapped(mov [copy("world"), copy("hello")], 0, 1))
|> iter/iter(fun s {println(s)})
