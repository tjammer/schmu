use iter

range(0, 10)
|> filter(fun i { (i |> mod(2)) == 0 })
|> take(5)
|> fold(mov 0, fun (mov acc, i) {
  print1("{}\n", int, i)
  acc + i
})
|>> fmt/(println(int))

let found = range(0, 10) |> any(fun x {
  fmt/(println(int, x))
  x == 3
})
fmt/(println(bool, found))

let found = range(0, 10) |> all(fun x {
  fmt/(println(int, x))
  mod(x, 2) == 0
})
fmt/(println(bool, found))
