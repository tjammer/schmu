module Stbl = Hashtbl.Make(String)

fun string():
    print("## string")
    let tbl& = Stbl.create(2)
    Stbl.insert(&tbl, !"key", !1.1)
    fun find_print(key):
        match Stbl.find(tbl, key):
            Some(v): print(fmt(v))
            None: print("none")
    find_print("key")
    find_print("otherkey")
    Stbl.remove(&tbl, "key")
    find_print("key")

print("# hashtbl")
string()
