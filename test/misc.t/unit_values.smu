-- variants
(def a ())
(def b (#some a))

(defn a [] ())
(def b (#some (a)))
(match b
  ((#some a) (print "some"))
  (#none (print "none")))

-- records
(type thing {:u unit})
(def t {:u ()})
(def u t.u)

(defn t [] {:u ()})
(def t2 (t))
(def u2 t2.u)

(type inrec {:i int :u unit :f float})
(def t {:i 10 :u () :f 99.9})
(ignore t.u)
(print (fmt-str t.f))

-- arrays
(def arr& [() ()])
(array/push &arr !())
(print (fmt-str (array/length arr)))
(ignore {:u arr.[2]})

-- fixed arrays
(def arr #[() ()])
(def u arr.(1))
(ignore {:u})
