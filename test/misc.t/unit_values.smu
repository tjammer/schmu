-- variants
let a = ()
let b = Some(a)

fun a(): ()
let b = Some(a())
match b:
  Some(a): print("some")
  None: print("none")

-- records
type thing = {u : unit}
let t = { u = () }
let u = t.u

fun t(): {u = ()}
let t2 = t()
let u2 = t2.u

type inrec = {i : int, u : unit, f : float}
let t = {i = 10, u = (), f = 99.9}
ignore(t.u)
print(fmt(t.f))

-- arrays
let arr& = [(), ()]
array/push(&arr, !())
print(fmt(array/length(arr)))
ignore({u = arr.[2]})

-- fixed arrays
let arr = #[(), ()]
let u = arr#[1]
ignore({u})
