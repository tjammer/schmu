fun true_() {
  println("true")
  true
}

fun false_() {
  println("false")
  false
}

println("test 'and':")

if true_() and true_() {
  println("yes")
} else {
  println("no")
}

if true_() and false_() {
  println("yes")
} else {
  println("no")
}

if false_() and true_() {
  println("yes")
} else {
  println("no")
}

if false_() and false_() {
  println("yes")
} else {
  println("no")
}

println("test 'or':")

if true_() or true_() {
  println("yes")
} else {
  println("no")
}

if true_() or false_() {
  println("yes")
} else {
  println("no")
}

if false_() or true_() {
  println("yes")
} else {
  println("no")
}

if false_() or false_() {
  println("yes")
} else {
  println("no")
}

println("test 'not':")

if not(true_()) {
  println("yes")
} else {
  println("no")
}

if not(false_()) {
  println("yes")
} else {
  println("no")
}
