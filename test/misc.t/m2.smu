fun rec even(i):
  match i:
    0: true
    1: false
    n: odd(n - 1)

and odd(i):
  match i:
    0: true
    1: true
    n: even(n - 1)

fun rec pop(arr&):
  println("pop")
  array/drop_back(&arr)
  pop_all(&arr)

and pop_all(arr&):
  if array/length(arr) == 0: ()
  else: pop(&arr)

