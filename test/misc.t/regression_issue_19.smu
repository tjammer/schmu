(type v3 {:x float :y float :z float})

(defn v3_add (lhs rhs)
  {:x (+. (.x lhs) (.x rhs)) :y (+. (.y lhs) (.y rhs)) :z (+. (.z lhs) (.z rhs))})

(defn v3_scale (v3 factor)
  {:x (*. (.x v3) factor) :y (*. (.y v3) factor) :z (*. (.z v3) factor)})

(defn wrap ()
  (-> (v3_scale {:x 1.0 :y 10.0 :z 100.0 } 1.5)
      (v3_add (v3_scale {:x 1.0 :y 2.0 :z 3.0} 1.5))))

(ignore (wrap))
