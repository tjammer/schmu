external puts : cstr -> unit

fun ps(str) =
  puts(str.cstr)

fun true_() =
  ps("true")
  true

fun false_() =
  ps("false")
  false

ps("test 'and':")

if true_() and true_() then ps("yes")
else ps("no")

if true_() and false_() then ps("yes")
else ps("no")

if false_() and true_() then ps("yes")
else ps("no")

if false_() and false_() then ps("yes")
else ps("no")

ps("test 'or':")

if true_() or true_() then ps("yes")
else ps("no")

if true_() or false_() then ps("yes")
else ps("no")

if false_() or true_() then ps("yes")
else ps("no")

if false_() or false_() then ps("yes")
else ps("no")

ps("test 'not':")

if true_() |. not then ps("yes")
else ps("no")

if false_() |. not then ps("yes")
else ps("no")
