(external printi (fun int unit))

(record int_wrap {:dat int :b int :c int})

(record (test 'a) {:int_wrap 'a})

(record (mut 'a) {:wrapped (mutable 'a)})

(fun test_thing []
  (val test {:int_wrap {:dat 2 :b 0 :c 0}})
  (fun vector_loop [i]
    (if (= i 10)
      (.int_wrap test)
      (vector_loop (+ i 1))))
  (vector_loop 0))

(fun test_thing_mut []
  (val test {:wrapped {:dat 2 :b 0 :c 0}})
  (fun vector_loop [i]
    (if (= i 10)
      ()
      (do
        (setf test .wrapped {:dat (+ (.dat (.wrapped test)) 1) :b 0 :c 0})
        (vector_loop (+ i 1)))))
  (vector_loop 0)
  (.wrapped test))

(-> (.dat (test_thing))
    printi)
(-> (.dat (test_thing_mut))
    printi)
